{
	"compilerOptions": {
		"baseUrl": "./",
		"target": "ES6",
		"strict": true,
		"module": "ES6",
		"esModuleInterop": true,
		"outDir": "./dist/",
		"moduleResolution":"node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": false
	},
	"include": [
		"src/**/*",
	],
	"exclude": [
		"node_modules"
	]
}