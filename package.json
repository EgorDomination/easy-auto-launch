{
	"name": "easy-auto-launch",
	"version": "6.0.3",
	"description": "Launch node applications or executables at login (Mac, Windows, and Linux)",
	"main": "dist/easy-auto-launch.js",
	"types": "dist/easy-auto-launch.d.ts",
	"scripts": {
		"build": "npm run clean && tsc",
		"postbuild": "copyfiles -f src/easy-auto-launch.d.ts dist",
		"clean": "del-cli dist",
		"prepare": "npm run build"
	},
	"engines": {
		"node": ">=4.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Izurii/easy-auto-launch"
	},
	"keywords": [
		"login",
		"launch",
		"node-webkit",
		"electron",
		"boot",
		"login-items"
	],
	"author": "Izurii Hootoh",
	"contributors": [],
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/Izurii/easy-auto-launch/issues"
	},
	"devDependencies": {
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^17.0.6",
		"@types/winreg": "^1.2.31",
		"del-cli": "^5.0.0",
		"typescript": "^4.5.4",
		"copyfiles": "^2.4.1"
	},
	"homepage": "https://github.com/Izurii/easy-auto-launch",
	"dependencies": {
		"applescript": "^1.0.0",
		"mkdirp": "^1.0.4",
		"untildify": "^4.0.0",
		"winreg": "^1.2.4"
	}
}
